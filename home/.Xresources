!!idk but pasted it)
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull


urxvt.termName: rxvt-unicode
urxvt.scrollBar: false
urxvt.background: black
urxvt.foreground: gray
!!Cursor Config
URxvt.cursorUnderline: true
URxvt.cursorBlink: true
!!Borders
URxvt.internalBorder:  10
!!Transparency
urxvt.transparent:	true
urxvt.shading:	20

!! CTRL +SHIFT  + C/V works
urxvt.keysym.Shift-Control-V: eval:paste_clipboard
urxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
!! Ctrl + SHift sh1t off
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Font prefferenes

#define termfont      xft:sourcecodepro:pixelsize=20:antialias=true
URxvt.font:           termfont
URxvt.boldFont:       termfont
URxvt.italicFont:     termfont
URxvt.boldItalicFont: termfont
urxvt.letterSpace: -1
!! Larger history limit
urxvt.saveLines: 1000

!! Perl extensions
urxvt.perl-ext-common: default,matcher,font-size
!! Open urls in browser with Control-Click
urxvt.urlLauncher: /usr/bin/chromium
urxvt.matcher.button: C1
!! Change font size on Control-Plus/Minus
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease

!!
urxvt.keysym.Home:          \033[1~
urxvt.keysym.End:           \033[4~
urxvt.keysym.Control-Up:    \033[1;5A
urxvt.keysym.Control-Down:  \033[1;5B
urxvt.keysym.Control-Left:  \033[1;5D
urxvt.keysym.Control-Right: \033[1;5C


!! Tango color theme
